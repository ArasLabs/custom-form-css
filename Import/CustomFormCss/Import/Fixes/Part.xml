          <![CDATA[<input type="button" name="major_rev_increment" value="+" style="width: 16px" onclick="createNewRevision();" />
<script type="text/javascript">
function createNewRevision()
{
  if(!top.aras.confirm(top.aras.getResource("plm", "createnewrevision.confirm")))
  {
    return;
  }
  var dom = top.aras.createXMLDocument();
  dom.loadXML(parent.item.xml);
  dom.documentElement.setAttribute('action', 'PE_CreateNewRevision');

  var res = top.aras.soapSend("ApplyMethod", dom.xml);
  if (res.getFaultCode() != 0)
  {
    top.aras.AlertError(res);
    return;
  }
  res = res.getResultsBody();
  dom.loadXML(res);

  top.aras.uiReShowItemEx(parent.itemID, dom.documentElement, parent.viewMode);
}
</script>]]>
